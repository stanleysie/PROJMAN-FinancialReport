<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="700.0" style="-fx-background-color: #ef5350;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Rectangle arcHeight="5.0" arcWidth="5.0" fill="WHITE" height="500.0" stroke="WHITE" strokeType="INSIDE" width="250.0" />
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="28.0" layoutY="175.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../files/logo.png" />
         </image>
      </ImageView>
      <Button fx:id="generate" layoutX="370.0" layoutY="140.0" mnemonicParsing="false" prefHeight="50.0" stylesheets="@Login.css" text="Generate New PDF" textFill="WHITE">
         <font>
            <Font name="Open Sans Light" size="20.0" />
         </font>
      </Button>
      <Button fx:id="load" layoutX="414.0" layoutY="215.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="115.0" stylesheets="@Login.css" text="Load File" textFill="WHITE">
         <font>
            <Font name="Open Sans Light" size="20.0" />
         </font>
      </Button>
      <Button fx:id="adminEdit" layoutX="382.0" layoutY="290.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="180.0" stylesheets="@Login.css" text="Admin Settings" textAlignment="CENTER" textFill="WHITE" wrapText="true">
         <font>
            <Font name="Open Sans Light" size="20.0" />
         </font>
      </Button>
      <Button fx:id="logout" layoutX="440.0" layoutY="455.0" mnemonicParsing="false" stylesheets="@Login.css" text="Log Out" textFill="WHITE">
         <font>
            <Font name="Open Sans Light" size="12.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
